        "Zamówienie opłacone",
        "Zamówienie zaakceptowane",
        "Zamówienie pakowane",
        "Zamówienie wysłane",
        "Zamówienie dostarczone"
      ],
      "default": "Zamówienie złożone",
      "required": false
    },
    "provider_option": {
      "type": "relation",
      "relation": "manyToOne",
