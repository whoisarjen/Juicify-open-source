      "inversedBy": "orders"
    },
    "email": {
      "type": "email",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "surname": {
      "type": "string",
      "required": true
    },
    "country": {
      "type": "string",
      "required": true
    },
    "postcode": {
      "type": "string",
      "required": true
    },
    "town": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "house": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "integer",
      "required": true,
      "min": 0
    }
  }
