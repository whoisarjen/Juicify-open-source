{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "manufacturer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::manufacturer.manufacturer",
      "inversedBy": "products"
    },
    "ean": {
      "type": "string"
    },
    "cn": {
      "type": "string"
    },
    "manufacturer_id": {
      "type": "string"
    },
    "warranty_in_months": {
      "type": "integer",
      "default": 0,
      "min": 0,
      "required": false
    },
    "recommended_products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "recommended_in_products"
    },
    "recommended_in_products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "recommended_products"
    },
    "manufacturer_net_price": {
      "type": "decimal",
      "required": true,
      "default": 0,
      "min": 0,
      "private": true
    },
    "manufacturer_gross_price": {
      "type": "decimal",
      "required": true,
      "default": 0,
      "min": 0,
      "private": true
    },
    "vat": {
      "type": "integer",
      "default": 23,
      "required": true,
      "min": 0
    },
    "gross_price": {
      "type": "decimal",
      "required": true,
      "default": 0,
      "min": 0
    },
    "net_price": {
      "type": "decimal",
      "required": true,
      "default": 0,
      "min": 0
    },
    "collection": {
      "type": "string"
    },
    "is_designed": {
      "type": "boolean"
    },
    "order_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-product.order-product",
      "mappedBy": "product"
    },
    "files": {
      "type": "json"
    },
    "package_height_in_mm": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "package_width_in_mm": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "package_depth_in_mm": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "package_weight_in_g": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "features": {
      "type": "json"
    },
    "properties": {
      "type": "json"
    },
    "finishes": {
      "type": "json"
    },
    "manufacturer_url": {
      "type": "string"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "source": {
      "type": "json"
    },
    "quantity": {
      "type": "integer",
      "required": false,
      "default": 0,
      "min": 0
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "products"
    }
  }
}
